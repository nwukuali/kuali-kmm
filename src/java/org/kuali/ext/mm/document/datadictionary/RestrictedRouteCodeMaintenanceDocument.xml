<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="StoresRestrictedRouteCodeMaintenanceDocument" parent="StoresRestrictedRouteCodeMaintenanceDocument-parentBean" />

  <bean id="StoresRestrictedRouteCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry"
        p:businessObjectClass="org.kuali.ext.mm.businessobject.RestrictedRouteCode"
        p:maintainableClass="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"
        p:documentTypeName="SRRC"
        p:documentAuthorizerClass="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" >
    <property name="lockingKeys" >
      <list>
        <value>restrictedRouteCd</value>
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
      </list>
    </property>
    <property name="workflowAttributes" >
        <ref bean="StoresRestrictedRouteCodeMaintenanceDocument-workflowAttributes" />
    </property>
    <property name="maintainableSections" >
      <list>
        <ref bean="StoresRestrictedRouteCodeMaintenanceDocument-MainSection" />
      </list>
    </property>
  </bean>

 <!-- Maintenance Section Definitions -->
  <bean id="StoresRestrictedRouteCodeMaintenanceDocument-MainSection" parent="StoresRestrictedRouteCodeMaintenanceDocument-MainSection-parentBean" />

  <bean id="StoresRestrictedRouteCodeMaintenanceDocument-MainSection-parentBean" abstract="true" parent="MaintainableSectionDefinition"
        p:title="Edit Restricted Route Code" >
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="restrictedRouteCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nm"
              p:required="true" />
		<bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />              
      </list>
    </property>
  </bean>
 <!-- workflow attributes -->
  <bean id="StoresRestrictedRouteCodeMaintenanceDocument-workflowAttributes" parent="StoresRestrictedRouteCodeMaintenanceDocument-workflowAttributes-parentBean" />

  <bean id="StoresRestrictedRouteCodeMaintenanceDocument-workflowAttributes-parentBean" abstract="true" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" />
</beans>
