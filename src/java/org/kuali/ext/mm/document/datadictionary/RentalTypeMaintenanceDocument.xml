<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="StoresRentalTypeMaintenanceDocument" parent="StoresRentalTypeMaintenanceDocument-parentBean" />

  <bean id="StoresRentalTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry"
        p:businessObjectClass="org.kuali.ext.mm.businessobject.RentalType"
        p:maintainableClass="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"
        p:documentTypeName="SRTP"
        p:documentAuthorizerClass="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" >
    <property name="lockingKeys" >
      <list>
        <value>rentalTypeCode</value>
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
      </list>
    </property>
    <property name="workflowAttributes" >
        <ref bean="StoresRentalTypeMaintenanceDocument-workflowAttributes" />
    </property>
    <property name="maintainableSections" >
      <list>
        <ref bean="StoresRentalTypeMaintenanceDocument-MainSection" />
      </list>
    </property>
  </bean>

 <!-- Maintenance Section Definitions -->
  <bean id="StoresRentalTypeMaintenanceDocument-MainSection" parent="StoresRentalTypeMaintenanceDocument-MainSection-parentBean" />

  <bean id="StoresRentalTypeMaintenanceDocument-MainSection-parentBean" abstract="true" parent="MaintainableSectionDefinition"
        p:title="Edit Rental Type" >
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="rentalTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="false" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
      </list>
    </property>
  </bean>
 <!-- workflow attributes -->
  <bean id="StoresRentalTypeMaintenanceDocument-workflowAttributes" parent="StoresRentalTypeMaintenanceDocument-workflowAttributes-parentBean" />

  <bean id="StoresRentalTypeMaintenanceDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes" />
</beans>
